    "query_last_order = \"\"\"\n",
    "SELECT \n",
    "    CASE \n",
    "        WHEN DaySinceLastOrder IS NULL THEN 'No data'\n",
    "        WHEN DaySinceLastOrder <= 14 THEN 'Recent (0-14 days)'\n",
    "        WHEN DaySinceLastOrder <= 30 THEN 'Medium (15-30 days)'\n",
    "        ELSE 'Long ago (30+ days)'\n",
    "    END AS recency,\n",
    "    COUNT(*) AS customers,\n",
    "    ROUND(AVG(Churn) * 100, 2) AS churn_rate\n",
    "FROM customers\n",
    "GROUP BY recency\n",
    "ORDER BY churn_rate DESC\n",
    "\"\"\"\n"
   ]
