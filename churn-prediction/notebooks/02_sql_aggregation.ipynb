    "query_satisfaction = \"\"\"\n",
    "SELECT \n",
    "    CASE \n",
    "        WHEN SatisfactionScore <= 2 THEN 'Low (1-2)'\n",
    "        WHEN SatisfactionScore = 3 THEN 'Medium (3)'\n",
    "        ELSE 'High (4-5)'\n",
    "    END AS satisfaction_level,\n",
    "    COUNT(*) AS customers,\n",
    "    ROUND(AVG(Churn) * 100, 2) AS churn_rate\n",
    "FROM customers\n",
    "GROUP BY satisfaction_level\n",
    "ORDER BY churn_rate DESC\n",
    "\"\"\""
   ]
