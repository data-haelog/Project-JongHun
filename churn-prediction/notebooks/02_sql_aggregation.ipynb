   "source": [
    "query_final = \"\"\"\n",
    "CREATE TABLE IF NOT EXISTS customer_features AS\n",
    "SELECT \n",
    "    CustomerID,\n",
    "    Churn,\n",
    "    \n",
    "    --  ( 0 )\n",
    "    COALESCE(Tenure, 0) AS Tenure,\n",
    "    COALESCE(OrderCount, 0) AS OrderCount,\n",
    "    COALESCE(DaySinceLastOrder, 0) AS DaySinceLastOrder,\n",
    "    COALESCE(CashbackAmount, 0) AS CashbackAmount,\n",
    "    SatisfactionScore,\n",
    "    Complain,\n",
    "    NumberOfDeviceRegistered,\n",
    "    CityTier,\n",
    "    \n",
    "    --  \n",
    "    CASE WHEN COALESCE(Tenure, 0) < 30 THEN 1 ELSE 0 END AS is_new,\n",
    "    CASE WHEN COALESCE(DaySinceLastOrder, 0) > 30 THEN 1 ELSE 0 END AS is_inactive,\n",
    "    CASE WHEN COALESCE(OrderCount, 0) < 2 THEN 1 ELSE 0 END AS low_orders,\n",
    "    CASE WHEN SatisfactionScore <= 2 THEN 1 ELSE 0 END AS dissatisfied,\n",
    "    CASE WHEN Complain = 1 THEN 1 ELSE 0 END AS has_complaint,\n",
    "    CASE WHEN CashbackAmount = 0 THEN 1 ELSE 0 END AS no_cashback\n",
    "    \n",
    "FROM customers\n",
    "\"\"\"\n",
    "\n",
    "conn.execute('DROP TABLE IF EXISTS customer_features')\n",
    "conn.execute(query_final)\n",
    "conn.commit()\n"
   ]
  },
