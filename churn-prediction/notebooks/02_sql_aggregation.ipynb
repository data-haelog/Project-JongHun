     "text": [
      "Risk Flag Impact on Churn:\n",
      "    flag_name  customers_with_flag  churn_rate_when_true\n",
      "  is_inactive                    2                 50.00\n",
      "has_complaint                 1604                 31.67\n",
      "       is_new                 5511                 17.20\n",
      "   low_orders                 2009                 16.63\n",
      " dissatisfied                 1750                 11.89\n",
      "  no_cashback                    4                  0.00\n",
      "\n",
      "Overall churn rate: 16.84%\n"
     ]
    }
   ],
   "source": [
    "flags = ['is_new', 'is_inactive', 'low_orders', 'dissatisfied', 'has_complaint', 'no_cashback']\n",
    "\n",
    "flag_impact = []\n",
    "for flag in flags:\n",
    "    query = f\"\"\"\n",
    "    SELECT \n",
    "        '{flag}' AS flag_name,\n",
    "        SUM({flag}) AS customers_with_flag,\n",
    "        ROUND(AVG(CASE WHEN {flag} = 1 THEN Churn ELSE NULL END) * 100, 2) AS churn_rate_when_true\n",
    "    FROM customer_features\n",
    "    \"\"\"\n",
    "    result = pd.read_sql_query(query, conn)\n",
    "    flag_impact.append(result)\n",
    "\n",
    "df_flag_impact = pd.concat(flag_impact, ignore_index=True)\n",
    "df_flag_impact = df_flag_impact.sort_values('churn_rate_when_true', ascending=False)\n",
    "\n",
    "print(\"Risk Flag Impact on Churn:\")\n",
    "print(df_flag_impact.to_string(index=False))\n",
    "\n",
    "overall_churn = df['Churn'].mean() * 100\n",
    "print(f\"\\nOverall churn rate: {overall_churn:.2f}%\")"
   ]
  },
