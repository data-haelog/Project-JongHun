   "source": [
    "query_basic = \"\"\"\n",
    "SELECT \n",
    "    Churn,\n",
    "    COUNT(*) AS customer_count,\n",
    "    AVG(Tenure) AS avg_tenure,\n",
    "    AVG(OrderCount) AS avg_orders,\n",
    "    AVG(SatisfactionScore) AS avg_satisfaction\n",
    "FROM customers\n",
    "GROUP BY Churn\n",
    "\"\"\""
   ]
